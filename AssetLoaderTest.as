package com.laz.util{	import flash.display.*;	import flash.events.*;	import flash.text.*;		public class AssetLoaderTest extends MovieClip	{		public var tProgress:TextField;		public var sProgress:String;		public var aLoader:AssetLoader;				public function AssetLoaderTest()		{			aLoader = new AssetLoader();			//loader must be told to addAssets in desired number order			//parameters are (url:*, oIndex:Object, mandatory:Boolean, self:Boolean = false, fComplete:Function = null, fProgress:Function = null, fError:Function = null)			//aLoader.addAssets(this, 1, false, true); //trying to check self-loaded percentile			aLoader.addAssets('moonfoot.swf', 1, true, false, draw);			aLoader.addAssets('moon.swf', 2, true, false, draw);			aLoader.addAssets('monster.swf', 3, false, false, draw);			aLoader.addAssets('city.swf', 4, false, false, draw);			aLoader.addAssets('marsimage.swf', 5, false, false, draw);			//aLoader.addAssets('fake.swf', 6, true, false);						aLoader.loadAssets();			 			progressDisplay();		}				public function progressDisplay()		{			tProgress = new TextField();			tProgress.textColor = 0xFFFFFF;			tProgress.y = 100;			tProgress.x = 100;			addChild(tProgress);						addEventListener(Event.ENTER_FRAME, updatePercent);		}				public function updatePercent(e:Event)		{			sProgress = aLoader.getPercent();			tProgress.text = "Loaded: " + sProgress + "%";		}				public function draw(e:Event):void		{			trace("aLoader.self:" + aLoader.self);			if(aLoader.self == false)			{			addChild(e.target.content);			}		}	}}